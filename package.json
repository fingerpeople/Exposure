    "tslint": "^5.17.0",
    "tslint-webpack-plugin": "^2.0.4",
    "typescript": "^3.5.2",
    "webpack": "^4.34.0",
    "webpack-cli": "^3.3.4",
    "webpack-merge": "^4.2.1"
